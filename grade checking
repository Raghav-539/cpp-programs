// check your grade using switch case

#include <iostream>
using namespace std;
  
int main()
{
    int cgpa;
    cout << "enter your cgpa : " ;
    cin >> cgpa;

    switch (cgpa)
    {
    case 10:
        cout<<"Outstanding";
        break;
    case 9:
        cout<<"Excellent";
        break;
    case 8:
        cout<<"Very Good";
        break;
    case 7:
        cout<<"Good";
        break;
    case 6:
        cout<<"Statisfied";
        break;
    case 5:
        cout<<"Accepted";
        break;

    default:
    cout<<"fail";
        break;
    }

    return 0;
}
